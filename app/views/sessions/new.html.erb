<h1>Welcome Back to Vendrio!</h1>

<div class="user_auth_form">
  <form action="<%= session_url %>" method="post">
    <%= auth_token %>

      <input class="user_name" type="text" name="user[username]" value="Guest">

      <input class="pass" type="password" name="user[password]" value="password">

    <input class="user_auth_button" type="submit" value="Sign In">
  </form><br>

  <div class="link">
    <%= link_to "Sign Up", new_user_url %>
  </div>
</div>
